local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Anti-Detection System
local function protectScript()
	-- Hook and protect against common anti-cheat detections
	local mt = getrawmetatable(game)
	local oldNamecall = mt.__namecall
	local oldIndex = mt.__index
	
	setreadonly(mt, false)
	
	-- Protect against remote detection
	mt.__namecall = newcclosure(function(self, ...)
		local method = getnamecallmethod()
		local args = {...}
		
		-- Block suspicious kick attempts
		if method == "Kick" then
			return
		end
		
		-- Hide our remote calls
		if method == "FireServer" or method == "InvokeServer" then
			-- Let legitimate calls through
			return oldNamecall(self, ...)
		end
		
		return oldNamecall(self, ...)
	end)
	
	-- Protect against property checks
	mt.__index = newcclosure(function(self, key)
		-- Hide suspicious properties
		if key == "Parent" and tostring(self) == "ModernUI" then
			return game:GetService("CoreGui")
		end
		
		return oldIndex(self, key)
	end)
	
	setreadonly(mt, true)
	
	-- Randomize timing to avoid pattern detection
	local random = Random.new()
	local function randomWait(min, max)
		wait(random:NextNumber(min or 0.1, max or 0.3))
	end
	
	return randomWait
end

local randomWait = protectScript()

-- Configuration
local UNLIMITED_JUMP_POWER = 50
local ORIGINAL_JUMP_POWER = 50
local TOTAL_PLOTS = 30

-- State
local unlimitedJumpEnabled = false
